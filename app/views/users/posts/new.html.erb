<h1>画像投稿フォーム</h1>
<%= form_with model: @post, url:posts_path, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>カテゴリー</h4>
  <%= f.collection_select :category_id, Category.all ,:id, :name, include_blank: "--選択してください--" %>
  <h4>説明</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>