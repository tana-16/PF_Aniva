<h1>編集画面</h1>
<!-- form_with部分 -->
<%= form_with model:@post, local: true do |f| %>
<%= f.attachment_field :image, placeholder: "プロフィール画像" %>
  <h4>カテゴリー</h4>
  <%= f.collection_select :category_id, Category.all ,:id, :name, include_blank: "--選択してください--" %>
  <h4>キャプション</h4>
  <%= f.text_area :body, autofocus: true, id:"inputbody" %>
  <%= f.submit '保存' %>
<% end %>